<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2_Share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:clickable="true"
        android:tint="#FFFFFF"
        app:backgroundTint="#F14914"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_share_24"
        tools:layout_editor_absoluteY="25dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton3_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_email_24"
        app:layout_constraintCircle="@+id/floatingActionButton2_Share"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintCircleAngle="180"
        tools:layout_editor_absoluteY="150dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:srcCompat="@drawable/ic_baseline_chat_24"
        app:layout_constraintCircle="@+id/floatingActionButton2_Share"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintCircleAngle="135"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="101dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton7_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:clickable="true"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_call_24"
        app:layout_constraintCircle="@+id/floatingActionButton2_Share"
        app:layout_constraintCircleRadius="100dp"
        app:layout_constraintCircleAngle="90"
        tools:layout_editor_absoluteX="153dp" />

</androidx.constraintlayout.widget.ConstraintLayout>